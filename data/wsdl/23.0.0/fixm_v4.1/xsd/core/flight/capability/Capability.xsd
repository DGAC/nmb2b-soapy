            </annotation>
         </enumeration>
         <enumeration value="B3">
            <annotation>
               <documentation>
                  RNAV 5 DME/DME 
               </documentation>
            </annotation>
         </enumeration>
         <enumeration value="B4">
            <annotation>
               <documentation>
                  RNAV 5 VOR/DME 
